{
  "name": "vite-react-webcomponents",
  "version": "0.1.0",
  "description": "The vite starter for create react components with web components shell.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/vite-react-webcomponents.d.ts",
  "author": "Joe_Sky <hitomi_doax@sina.com>",
  "files": [
    "cjs/",
    "esm/",
    "src/",
    "types/vite-react-webcomponents.d.ts",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "dev:wc": "cross-env WC=true vite",
    "build-web": "vite build",
    "build-web:wc": "cross-env WC=true vite build",
    "esm": "cross-env NODE_ENV=production rollup -c build/rollup.config.js -o esm/vite-react-webcomponents.js && npm run esm:index && npm run dts",
    "esm:index": "babel src/components/entry/index.tsx -o esm/index.js --extensions \".js,.jsx,.ts,.tsx\" --config-file \"./build/.babelrc\"",
    "dts": "rollup -c build/rollup.config.dts.js -o types/vite-react-webcomponents.d.ts",
    "style": "rollup -c build/rollup.config.style.js -o style/useless.js"
  },
  "dependencies": {
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@pika/react": "file:./node_modules_local/react",
    "@pika/react-dom": "file:./node_modules_local/react-dom",
    "antd": "^4.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.161",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@vue/runtime-dom": "file:./node_modules_local/@vue/runtime-dom",
    "babel-plugin-import": "^1.13.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "less": "^3.12.2",
    "prettier": "^2.1.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.3",
    "vite": "^1.0.0-rc.13",
    "vite-plugin-react": "^3.0.0"
  }
}
