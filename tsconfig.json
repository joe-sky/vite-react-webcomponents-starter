{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "target": "es2019",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./built",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitThis": true
  },
  "include": ["src", "types"],
  "exclude": ["src/components/entry/index.tsx", "src/components/entry/vue.ts"]
}
